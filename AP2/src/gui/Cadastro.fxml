<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CadastroViewController">
   <children>
      <Label alignment="TOP_LEFT" prefHeight="0.0" prefWidth="514.0" text="Cadastrar">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding>
      </Label>
      <HBox prefHeight="0.0" prefWidth="514.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Nome">
               <HBox.margin>
                  <Insets right="19.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldName" />
         </children>
         <padding>
            <Insets left="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="514.0">
         <children>
            <Label text="Email">
               <HBox.margin>
                  <Insets right="23.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldEmail" />
         </children>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <HBox prefHeight="11.0" prefWidth="514.0">
         <children>
            <Label text="CPF">
               <HBox.margin>
                  <Insets right="32.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldCPF" />
         </children>
         <padding>
            <Insets left="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="514.0">
         <children>
            <Label text="Altura">
               <HBox.margin>
                  <Insets right="22.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldAltura" />
         </children>
         <padding>
            <Insets left="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="514.0">
         <children>
            <Label text="Idade">
               <HBox.margin>
                  <Insets right="24.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldIdade" />
         </children>
         <padding>
            <Insets left="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="514.0">
         <children>
            <Label text="Endereço">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldEndereco" />
         </children>
         <padding>
            <Insets left="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="514.0">
         <children>
            <Label text="Número">
               <HBox.margin>
                  <Insets right="11.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="textFieldNumero" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" top="5.0" />
         </padding>
      </HBox>
      <Button fx:id="btnCadastrar" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onCadastrarAction" onMouseClicked="#onCadastrarAction" text="Cadastrar">
         <VBox.margin>
            <Insets bottom="10.0" left="100.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
